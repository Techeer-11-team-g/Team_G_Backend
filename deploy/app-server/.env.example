# Django
SECRET_KEY=your-secret-key-here
DEBUG=False
ALLOWED_HOSTS=your-domain.com,app-server-ip

# MySQL (Cloud SQL)
DB_NAME=team_g_db
DB_USER=your-db-user
DB_PASSWORD=your-db-password
DB_HOST=CLOUD_SQL_PRIVATE_IP  # Cloud SQL 내부 IP
DB_PORT=3306

# Redis (Queue Server IP)
REDIS_HOST=QUEUE_SERVER_PRIVATE_IP
REDIS_PORT=6379

# RabbitMQ (Queue Server IP)
RABBITMQ_HOST=QUEUE_SERVER_PRIVATE_IP
RABBITMQ_PORT=5672
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=your-rabbitmq-password

# OpenSearch (Search Server IP)
OPENSEARCH_HOST=SEARCH_SERVER_PRIVATE_IP
OPENSEARCH_PORT=9200
OPENSEARCH_USER=admin
OPENSEARCH_PASSWORD=your-opensearch-password
OPENSEARCH_USE_SSL=False

# OpenAI / LangChain
OPENAI_API_KEY=your-openai-api-key
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=

# Google Cloud Storage
GCS_BUCKET_NAME=your-bucket-name
GCS_PROJECT_ID=your-project-id
GCS_CREDENTIALS_FILE=/app/gcs-credentials.json
MEDIA_URL=https://storage.googleapis.com/your-bucket-name/

# Logging
DJANGO_LOG_LEVEL=INFO
